<?xml version="1.0"?>
<launch>
<arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>
<arg name="nodelet_manager" default="camera_nodelet_manager"/>

<node pkg="nodelet" type="nodelet" name="cam3d_throttle"  args="load cob_camera_sensors/Cam3DThrottle $(arg nodelet_manager)" output="screen">
	<remap from="cloud_in" to="/cam3d/rgb/points"/>
	<remap from="cloud_out" to="/cam3d/rgb/points_throttled"/>
</node>
</launch>

