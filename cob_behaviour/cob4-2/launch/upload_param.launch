<?xml version="1.0"?>
<launch>

	<!-- send ROBOT parameters to parameter server -->
	<rosparam command="load" ns="/script_server/arm_left" file="$(find cob_behaviour)/cob4-2/config/arm_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/arm_right" file="$(find cob_behaviour)/cob4-2/config/arm_right_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/torso" file="$(find cob_behaviour)/cob4-2/config/torso_joint_configurations.yaml"/>

</launch>
